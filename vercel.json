{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "corpready-ai-n8n",
  "buildCommand": "cd coursera-path-main && npm install && npm run build",
  "outputDirectory": "coursera-path-main/dist",
  "devCommand": "cd coursera-path-main && npm run dev",
  "installCommand": "cd coursera-path-main && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
